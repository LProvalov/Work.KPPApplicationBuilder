<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Colors -->
    <Color x:Key="ColorPrimary01">#bbbbff</Color>
    <Color x:Key="ColorPrimary02">#7777ff</Color>
    
    <!-- Brushes -->
    <SolidColorBrush x:Key="BrushPrimary01" Color="{StaticResource ColorPrimary01}"></SolidColorBrush>
    <SolidColorBrush x:Key="BrushPrimary02" Color="{StaticResource ColorPrimary02}"></SolidColorBrush>
    
</ResourceDictionary>